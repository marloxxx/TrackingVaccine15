@model TrackingVaccineClient.ProcedurService.VaccineUsage[]
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_LayoutBackend.cshtml";
}

<main>
    <header class="page-header page-header-dark bg-gradient-primary-to-secondary pb-10">
        <div class="container-xl px-4">
            <div class="page-header-content pt-4">
                <div class="row align-items-center justify-content-between">
                    <div class="col-auto mt-4">
                        <h1 class="page-header-title">
                            <div class="page-header-icon">
                                <i data-feather="activity"></i>
                            </div>
                            List Vaccine
                        </h1>
                        <div class="page-header-subtitle">
                            List Vaccine
                        </div>
                    </div>
                    <div class="col-12 col-xl-auto mt-4">
                        <div class="btn-group">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Main page content-->
    <div class="container-xl px-4 mt-n10">
        <div class="card mb-4">
            <div class="card-header">List Vaccine</div>
            <div class="card-body">
                <table id="datatables" class="table" style="width: 100%">
                    <thead>
                        <tr>
                            <th>Registered Number Vaccine</th>
                            <th>NIK</th>
                            <th>Used At</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.vaccine_code</td>
                                <td>@item.resident_nik</td>
                                <td>@item.used_at</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</main>
@section scripts{
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    @if (Session["success"] != null)
    {
        <script>
            swal({
                title: "Success",
                text: "@Session["success"]",
                icon: "success",
                button: "ok"
            }).then((value) => {
                $.ajax({
                    url: "/Producer/RemoveSession",
                    type: "GET",
                    success: function (response) {
                        console.log(response);
                    }
                })
            });
        </script>
    }
}
