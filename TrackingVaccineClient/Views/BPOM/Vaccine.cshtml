@model TrackingVaccineClient.ProcedurService.Vaccine[]
@{
    ViewBag.Title = "Vaccine";
}
<!-- Breadcrumb -->
<section class="bg-half-170 d-table w-100 bg-light" style="background: url('images/bg/page.png') top center;"
         id="home">
    <!-- container -->
    <div class="container">
        <!-- row -->
        <div class="row mt-5">
            <!-- /col -->
            <div class="col-lg-12">
                <div class="title-heading text-center text-md-center">
                    <h3>Vaccine</h3>
                    <nav aria-label="breadcrumb" class="d-inline-block mt-2">
                        <ul class="breadcrumb mb-0">
                            <li class="breadcrumb-item"><a href="index-2.html">BPOM</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Vaccine</li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /col -->
        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</section>
<!-- /Breadcrumb -->
<!--================Data Table Area =================-->
<section class="data-table-area">
    <div class="container">
        <div class="main_title text-center">
            <h5>All Registered Vaccine</h5>
        </div>
        <!-- button validate -->
        <div class="row mb-3">
            <div class="col-lg-12">
                <div class="button validate">
                    <a href="Validate" class="btn theme-btn">
                        Validate
                    </a>
                </div>
            </div>
        </div>
        @if (Model != null)
        {
            <div class="all-country table-responsive">
            <table id="world_table" class="table table-striped table-bordered dataTable no-footer" role="grid"
                   aria-describedby="world_table_info">
                <thead>
                    <tr role="row">
                        <th>ID Vaccine</th>
                        <th>Nama</th>
                        <th>Registered Number</th>
                        <th>Registered Date</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr role="row">
                            <td>@item.id</td>
                            <td>@item.code</td>
                            <td>@item.registered_number</td>
                            <td>@item.registered_date</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        } else
        {
            <div class="all-country table-responsive">
                <h3>No Data</h3>
            </div>
        }
    </div>
</section>
<!--================End Data Table Area =================-->